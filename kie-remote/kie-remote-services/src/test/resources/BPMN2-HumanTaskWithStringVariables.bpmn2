        <bpmn2:resourceAssignmentExpression id="_task-2_pot_owner_assign">
          <bpmn2:formalExpression id="_task-2_pot_owner_assign_expr">john</bpmn2:formalExpression>
        </bpmn2:resourceAssignmentExpression>
